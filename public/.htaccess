<IfModule mod_proxy.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^api(.*)$ https://api.binance.com/api$1 - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>